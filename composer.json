{
    "name": "znmin/laravel-deployer",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "symfony/process": "^4.0",
        "illuminate/support": "^5.5|^6.0",
        "illuminate/routing": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Znmin\\LaravelDeployer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Znmin\\LaravelDeployer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Znmin\\LaravelDeployer\\ServiceProvider"
            ]
        }
    }
}
